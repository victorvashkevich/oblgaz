	СписокСчетовБухУчета.Добавить(ПланыСчетов.Хозрасчетный.РасходыНаОплатуТрудаБудущихПериодов);
	
	Запрос.УстановитьПараметр("СчетБУ",		СписокСчетовБухУчета);
	Запрос.УстановитьПараметр("Счет97",		ПланыСчетов.Хозрасчетный.РасходыНаОплатуТрудаБудущихПериодов);
		
КонецЕсли;

РезультатЗапроса = Запрос.Выполнить();
Если РезультатЗапроса.Пустой() Тогда
	Отказ = Истина
КонецЕсли;

// Структура документа
ТабДок = Новый ТаблицаЗначений;
ТабДок.Колонки.Добавить("ДатаДок");
ТабДок.Колонки.Добавить("НомерДок");
ТабДок.Колонки.Добавить("Комментарий");
ТабДок.Колонки.Добавить("Ссылка");
ТабДок.Колонки.Добавить("ТабличнаяЧасть");

Выборка = РезультатЗапроса.Выбрать();
Пока Выборка.СледующийПоЗначениюПоля("Ссылка") Цикл

	НоваяСтрока = ТабДок.Добавить();
	НоваяСтрока.ДатаДок		= Выборка.ДатаДок;
	НоваяСтрока.НомерДок	= Выборка.Номер;
	НоваяСтрока.Комментарий	= Выборка.Комментарий;
	НоваяСтрока.Ссылка		= Выборка.Ссылка;
	
	НоваяСтрока.ТабличнаяЧасть	= Новый ТаблицаЗначений;
	//НоваяСтрока.ТабличнаяЧасть.Колонки.Добавить("НомерСтроки");
	НоваяСтрока.ТабличнаяЧасть.Колонки.Добавить("ДебетСчет");
	НоваяСтрока.ТабличнаяЧасть.Колонки.Добавить("ДебетСубконто1");
	НоваяСтрока.ТабличнаяЧасть.Колонки.Добавить("ДебетСубконто2");
	НоваяСтрока.ТабличнаяЧасть.Колонки.Добавить("ДебетСубконто3");
	НоваяСтрока.ТабличнаяЧасть.Колонки.Добавить("КредитСчет");
	НоваяСтрока.ТабличнаяЧасть.Колонки.Добавить("КредитСубконто1");
	НоваяСтрока.ТабличнаяЧасть.Колонки.Добавить("КредитСубконто2");
	НоваяСтрока.ТабличнаяЧасть.Колонки.Добавить("КредитСубконто3");
	НоваяСтрока.ТабличнаяЧасть.Колонки.Добавить("Сумма");

	Пока Выборка.Следующий() Цикл

		НоваяСТрокаТЧ = НоваяСтрока.ТабличнаяЧасть.Добавить();
		//НоваяСТрокаТЧ.НомерСтроки		= Выборка.НомерСтроки;
		НоваяСТрокаТЧ.ДебетСчет			= Выборка.СчетДт;
		
		// У этих счетов субконто не совпадает
		Если Выборка.СчетДт = ПланыСчетов.Хозрасчетный.ОбщехозяйственныеРасходы Или
			Выборка.СчетДт = ПланыСчетов.Хозрасчетный.ОбщепроизводственныеРасходы Тогда
			
			НоваяСТрокаТЧ.ДебетСубконто1	= Выборка.СубконтоДт2;
			НоваяСТрокаТЧ.ДебетСубконто2	= Выборка.СубконтоДт1;
			НоваяСТрокаТЧ.ДебетСубконто3	= Выборка.СубконтоДт3;
			
		ИначеЕсли Выборка.СчетДт = ПланыСчетов.Хозрасчетный.ОсновноеПроизводство 
			ИЛИ Выборка.СчетДт = ПланыСчетов.Хозрасчетный.БракВПроизводстве Тогда
			
			НоваяСТрокаТЧ.ДебетСубконто1	= Выборка.СубконтоДт2;
			НоваяСТрокаТЧ.ДебетСубконто2	= Выборка.СубконтоДт3;
			НоваяСТрокаТЧ.ДебетСубконто3	= Выборка.СубконтоДт1;
			
		Иначе
			
			НоваяСТрокаТЧ.ДебетСубконто1	= Выборка.СубконтоДт1;
			НоваяСТрокаТЧ.ДебетСубконто2	= Выборка.СубконтоДт2;
			НоваяСТрокаТЧ.ДебетСубконто3	= Выборка.СубконтоДт3;

		КонецЕсли;
		
		НоваяСТрокаТЧ.КредитСчет		= Выборка.СчетКт;
		НоваяСТрокаТЧ.КредитСубконто1	= Выборка.СубконтоКт1;
		НоваяСТрокаТЧ.КредитСубконто2	= Выборка.СубконтоКт2;
		НоваяСТрокаТЧ.КредитСубконто3	= Выборка.СубконтоКт3;
		НоваяСТрокаТЧ.Сумма				= Выборка.Сумма;
	КонецЦикла;
	
КонецЦикла;

ВыборкаДанных = ТабДок;</ПередОбработкойПравила></Правило></ПравилаВыгрузкиДанных><ПравилаОчисткиДанных/><Алгоритмы><Алгоритм Имя="ВидНалогаПоСчету"><Текст>// Определение соответствия элемента справочника "Налоги и отчисления" из Бухгалтерии 7.7
// по счету учета налогов из ЗУП 2.1
//
// ЗначениеИсточник - ПланыСчетов.Хозрасчетный, счет дебета или кредита проводки.
// ЗначениеПриемник - Строка, возвращаемое значение, код предорпределенного элемента справочника "Налоги и отчисления"
//
Если ЗначениеИсточник = ПланыСчетов.Хозрасчетный.ПФР_нак Тогда
	ЗначениеПриемник = "ПФР_нак"
ИначеЕсли ЗначениеИсточник = ПланыСчетов.Хозрасчетный.ПФР_страх Тогда
	ЗначениеПриемник = "ПФР_страх"
ИначеЕсли ЗначениеИсточник = ПланыСчетов.Хозрасчетный.ФСС_ДобровольныеВзносы Тогда
	ЗначениеПриемник = "ФСС_БЛ"
ИначеЕсли ЗначениеИсточник = ПланыСчетов.Хозрасчетный.ФСС_НСиПЗ Тогда
	ЗначениеПриемник = "ФСС_НС"
ИначеЕсли ЗначениеИсточник = ПланыСчетов.Хозрасчетный.ТФОМС Тогда
	ЗначениеПриемник = "ТФОМС"
ИначеЕсли ЗначениеИсточник = ПланыСчетов.Хозрасчетный.ФФОМС Тогда
	ЗначениеПриемник = "ФФОМС"
ИначеЕсли ЗначениеИсточник = ПланыСчетов.Хозрасчетный.ФСС Тогда
	ЗначениеПриемник = "ФСС"
ИначеЕсли ЗначениеИсточник = ПланыСчетов.Хозрасчетный.ЕСН_ФБ Тогда
	ЗначениеПриемник = "ЕСН_ФБ"
КонецЕсли;</Текст></Алгоритм><Алгоритм Имя="ПреобразоватьСубконтоПоТипуЗначения"><Текст>// По типу субконто определим правило конвертации.
// ЗначениеИсточник	- Тип составной, значение субконто, тип которого нужно определить
// ИсточникСчет		- ПланыСчетов.Хозрасчетный, счет дебета или кредита проводки.
// ИмяПКО			- Строка, возвращаемое значение - имя ПКО.
//
// Определяем тип субконто. В зависимости от типа субконта и в некоторых случаях от счета проводки
// определяем имя ПКО по которому это значение будет конвертироваться.
//
Если ТипЗнч(ЗначениеИсточник) = Тип("СправочникСсылка.ПодразделенияОрганизаций") Тогда
	ИмяПКО = "Подразделения"
ИначеЕсли ТипЗнч(ЗначениеИсточник) = Тип("СправочникСсылка.РасходыБудущихПериодов") Тогда
	ИмяПКО = "РасходыБудущихПериодов"
ИначеЕсли ТипЗнч(ЗначениеИсточник) = Тип("СправочникСсылка.НоменклатурныеГруппы") Тогда
	ИмяПКО = "ВидыНоменклатуры"
ИначеЕсли ТипЗнч(ЗначениеИсточник) = Тип("СправочникСсылка.СтатьиЗатрат") Тогда
	Если ИсточникСчет.ПринадлежитЭлементу(ПланыСчетов.Хозрасчетный.ОсновноеПроизводство_) 
		ИЛИ ИсточникСчет = ПланыСчетов.Хозрасчетный.БракВПроизводстве Тогда
		ИмяПКО = "СтатьиЗатратНаПроизводство"
	ИначеЕсли ИсточникСчет = ПланыСчетов.Хозрасчетный.ОбщехозяйственныеРасходы Тогда
		ИмяПКО = "СтатьиОбщехозяйственныхЗатрат"
	ИначеЕсли ИсточникСчет = ПланыСчетов.Хозрасчетный.ОбщепроизводственныеРасходы Тогда
		ИмяПКО = "СтатьиОбщехозяйственныхЗатрат"
	ИначеЕсли ИсточникСчет = ПланыСчетов.Хозрасчетный.ИздержкиОбращения Тогда
		ИмяПКО = "СтатьиИздержекОбращения"
	ИначеЕсли ИсточникСчет = ПланыСчетов.Хозрасчетный.КоммерческиеРасходы Тогда
		ИмяПКО = "СтатьиКоммерческихРасходов"
	ИначеЕсли ИсточникСчет.ПринадлежитЭлементу(ПланыСчетов.Хозрасчетный.ВложенияВоВнеоборотныеАктивы) Тогда
		ИмяПКО = "СтатьиЗатратНаСтроительство"
	КонецЕсли;	
ИначеЕсли ТипЗнч(ЗначениеИсточник) = Тип("ПеречислениеСсылка.ВидыПлатежейВГосБюджет") Тогда
	Если ИсточникСчет = ПланыСчетов.Хозрасчетный.ФСС 
		ИЛИ ИсточникСчет = ПланыСчетов.Хозрасчетный.ФСС_НСиПЗ 
		ИЛИ ИсточникСчет = ПланыСчетов.Хозрасчетный.ФСС_ДобровольныеВзносы Тогда
		ИмяПКО = "ВидыПлатежейВФонды"
	Иначе
		ИмяПКО = "ВидыПлатежейВБюджет"
	КонецЕсли;	
ИначеЕсли ТипЗнч(ЗначениеИсточник) = Тип("СправочникСсылка.ФизическиеЛица") Тогда
		ИмяПКО = "Сотрудники"
ИначеЕсли ТипЗнч(ЗначениеИсточник) = Тип("СправочникСсылка.Контрагенты") Тогда
		ИмяПКО = "Контрагенты"
ИначеЕсли ТипЗнч(ЗначениеИсточник) = Тип("СправочникСсылка.РасходыБудущихПериодов") Тогда
		ИмяПКО = "РасходыБудущихПериодов"
ИначеЕсли ТипЗнч(ЗначениеИсточник) = Тип("СправочникСсылка.ПрочиеДоходыИРасходы") Тогда
		ИмяПКО = "ПрочиеДоходыИРасходы"
ИначеЕсли ТипЗнч(ЗначениеИсточник) = Тип("СправочникСсылка.ДоговорыКонтрагентов") Тогда
		ИмяПКО = "Договоры"
ИначеЕсли ТипЗнч(ЗначениеИсточник) = Тип("СправочникСсылка.Резервы") Тогда
		ИмяПКО = "Резервы"
ИначеЕсли ТипЗнч(ЗначениеИсточник) = Тип("СправочникСсылка.ОбъектыСтроительства") Тогда
		ИмяПКО = "ОбъектыСтроительства"
ИначеЕсли ТипЗнч(ЗначениеИсточник) = Тип("СправочникСсылка.НематериальныеАктивы") Тогда
		ИмяПКО = "НМАОбъектыСтроительства"
КонецЕсли;</Текст></Алгоритм></Алгоритмы><Запросы><Запрос Имя="ОтражениеЗарплатыВРеглУчете"><Текст>ВЫБРАТЬ
	ОтражениеЗарплатыВРеглУчетеОтражениеВУчете.СчетКт,
	ОтражениеЗарплатыВРеглУчетеОтражениеВУчете.СчетДт,
	ОтражениеЗарплатыВРеглУчетеОтражениеВУчете.Ссылка.Дата	КАК ДатаДок,
	ОтражениеЗарплатыВРеглУчетеОтражениеВУчете.Ссылка.Номер КАК Номер,
	ПОДСТРОКА(ОтражениеЗарплатыВРеглУчетеОтражениеВУчете.Ссылка.Комментарий, 1, 500) КАК Комментарий,
	ОтражениеЗарплатыВРеглУчетеОтражениеВУчете.Ссылка.Ссылка КАК Ссылка,
	ОтражениеЗарплатыВРеглУчетеОтражениеВУчете.СубконтоДт1,
	ОтражениеЗарплатыВРеглУчетеОтражениеВУчете.СубконтоДт2,
	СУММА(ОтражениеЗарплатыВРеглУчетеОтражениеВУчете.Сумма) КАК Сумма
ИЗ
	Документ.ОтражениеЗарплатыВРеглУчете.ОтражениеВУчете КАК ОтражениеЗарплатыВРеглУчетеОтражениеВУчете
ГДЕ
	ОтражениеЗарплатыВРеглУчетеОтражениеВУчете.Ссылка.Проведен = &amp;Проведен
	И ОтражениеЗарплатыВРеглУчетеОтражениеВУчете.Ссылка.Организация = &amp;Организация
//Доп условие	
	
	СГРУППИРОВАТЬ ПО
		ОтражениеЗарплатыВРеглУчетеОтражениеВУчете.СчетКт,
		ОтражениеЗарплатыВРеглУчетеОтражениеВУчете.СчетДт,
		ОтражениеЗарплатыВРеглУчетеОтражениеВУчете.Ссылка.Дата,
		ОтражениеЗарплатыВРеглУчетеОтражениеВУчете.Ссылка.Номер,
		ОтражениеЗарплатыВРеглУчетеОтражениеВУчете.Ссылка.Ссылка,
		ОтражениеЗарплатыВРеглУчетеОтражениеВУчете.СубконтоДт1,
		ОтражениеЗарплатыВРеглУчетеОтражениеВУчете.СубконтоДт2
	
УПОРЯДОЧИТЬ ПО
	Ссылка,
	СубконтоДт1</Текст></Запрос><Запрос Имя="ОтражениеЗарплатыВРеглУчетеБУ"><Текст>ВЫБРАТЬ
	ОтражениеЗарплатыВРеглУчетеОтражениеВУчете.Ссылка.Ссылка КАК Ссылка,
	ОтражениеЗарплатыВРеглУчетеОтражениеВУчете.Ссылка.Дата КАК ДатаДок,
	ОтражениеЗарплатыВРеглУчетеОтражениеВУчете.Ссылка.Номер КАК Номер,
	ОтражениеЗарплатыВРеглУчетеОтражениеВУчете.Ссылка.Комментарий КАК Комментарий,
	ОтражениеЗарплатыВРеглУчетеОтражениеВУчете.СчетДт,
	ОтражениеЗарплатыВРеглУчетеОтражениеВУчете.СубконтоДт1,
	ОтражениеЗарплатыВРеглУчетеОтражениеВУчете.СубконтоДт2,
	ОтражениеЗарплатыВРеглУчетеОтражениеВУчете.СубконтоДт3,
	ОтражениеЗарплатыВРеглУчетеОтражениеВУчете.СчетКт,
	ОтражениеЗарплатыВРеглУчетеОтражениеВУчете.СубконтоКт1,
	ОтражениеЗарплатыВРеглУчетеОтражениеВУчете.СубконтоКт2,
	ОтражениеЗарплатыВРеглУчетеОтражениеВУчете.СубконтоКт3,
	ОтражениеЗарплатыВРеглУчетеОтражениеВУчете.НомерСтроки,
	ОтражениеЗарплатыВРеглУчетеОтражениеВУчете.Сумма
ИЗ
	Документ.ОтражениеЗарплатыВРеглУчете.ОтражениеВУчете КАК ОтражениеЗарплатыВРеглУчетеОтражениеВУчете
ГДЕ
	ОтражениеЗарплатыВРеглУчетеОтражениеВУчете.Ссылка.Проведен = &amp;Проведен
	И ОтражениеЗарплатыВРеглУчетеОтражениеВУчете.Ссылка.Организация = &amp;Организация
	И ОтражениеЗарплатыВРеглУчетеОтражениеВУчете.Сумма &lt;&gt; 0
//Доп условие

УПОРЯДОЧИТЬ ПО
	Ссылка,
	НомерСтроки</Текст></Запрос><Запрос Имя="ОтражениеЗарплатыВРеглУчетеБУ_Сводно"><Текст>ВЫБРАТЬ
	ОтражениеЗарплатыВРеглУчетеОтражениеВУчете.Ссылка.Ссылка КАК Ссылка,
	ОтражениеЗарплатыВРеглУчетеОтражениеВУчете.Ссылка.Дата КАК ДатаДок,
	ОтражениеЗарплатыВРеглУчетеОтражениеВУчете.Ссылка.Номер КАК Номер,
	ПОДСТРОКА(ОтражениеЗарплатыВРеглУчетеОтражениеВУчете.Ссылка.Комментарий, 1, 500) КАК Комментарий,
	ОтражениеЗарплатыВРеглУчетеОтражениеВУчете.СчетДт,
	ВЫБОР
		КОГДА ОтражениеЗарплатыВРеглУчетеОтражениеВУчете.СчетДт В (&amp;СчетБУ)
			ТОГДА ВЫБОР
					КОГДА ОтражениеЗарплатыВРеглУчетеОтражениеВУчете.СчетДт &lt;&gt; &amp;Счет97
						ТОГДА "ПоВсемРаботникам"
					ИНАЧЕ ОтражениеЗарплатыВРеглУчетеОтражениеВУчете.СубконтоДт1
				КОНЕЦ
		ИНАЧЕ ОтражениеЗарплатыВРеглУчетеОтражениеВУчете.СубконтоДт1
	КОНЕЦ КАК СубконтоДт1,
	ВЫБОР
		КОГДА ОтражениеЗарплатыВРеглУчетеОтражениеВУчете.СчетДт = &amp;Счет97
			ТОГДА "ПоВсемРаботникам"
		ИНАЧЕ ОтражениеЗарплатыВРеглУчетеОтражениеВУчете.СубконтоДт2
	КОНЕЦ КАК СубконтоДт2,
	СубконтоДт3,
	ОтражениеЗарплатыВРеглУчетеОтражениеВУчете.СчетКт,
	ВЫБОР
		КОГДА ОтражениеЗарплатыВРеглУчетеОтражениеВУчете.СчетКт В (&amp;СчетБУ)
			ТОГДА ВЫБОР
					КОГДА ОтражениеЗарплатыВРеглУчетеОтражениеВУчете.СчетКт &lt;&gt; &amp;Счет97
						ТОГДА "ПоВсемРаботникам"
					ИНАЧЕ ОтражениеЗарплатыВРеглУчетеОтражениеВУчете.СубконтоКт1
				КОНЕЦ
		ИНАЧЕ ОтражениеЗарплатыВРеглУчетеОтражениеВУчете.СубконтоКт1
