	|	СУММА(ЕСТЬNULL(ОсновныеНачисления.Пособия, 0) + ЕСТЬNULL(ДополнительныеНачисления.Пособия, 0)) КАК МатПомощь,
	|	СУММА(ЕСТЬNULL(Налог.ПН, 0)) КАК ПН,
