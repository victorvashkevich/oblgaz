			<dataPath>КодПричины</dataPath>
			<field>КодПричины</field>
		</field>
		<dataSource>ИнформационнаяБаза</dataSource>
		<query>ВЫБРАТЬ РАЗРЕШЕННЫЕ
	Организации.Ссылка КАК Организация
ПОМЕСТИТЬ ВТОрганизации
ИЗ
	Справочник.Организации КАК Организации
;

////////////////////////////////////////////////////////////////////////////////
ВЫБРАТЬ
	НачислениеПоБольничномуЛисту.Организация,
	НачислениеПоБольничномуЛисту.Сотрудник,
	НачислениеПоБольничномуЛисту.ДатаНачала,
	НачислениеПоБольничномуЛисту.ДатаОкончания,
	НачислениеПоБольничномуЛисту.Ссылка,
	ВЫБОР
		КОГДА НачислениеПоБольничномуЛисту.Диагноз.Родитель=ЗНАЧЕНИЕ(Справочник.КлассификаторДиагнозов.ПустаяСсылка)
			ТОГДА НачислениеПоБольничномуЛисту.Диагноз.Наименование
		ИНАЧЕ НачислениеПоБольничномуЛисту.Диагноз.Родитель.Наименование
	КОНЕЦ КАК Диагноз,
	ВЫБОР
		КОГДА НачислениеПоБольничномуЛисту.Диагноз.Родитель=ЗНАЧЕНИЕ(Справочник.КлассификаторДиагнозов.ПустаяСсылка)
			ТОГДА НачислениеПоБольничномуЛисту.Диагноз.Код
		ИНАЧЕ НачислениеПоБольничномуЛисту.Диагноз.Родитель.Код
	КОНЕЦ КАК КодПричины,
	НачислениеПоБольничномуЛисту.Диагноз.Код КАК КодДиагноза,
	НачислениеПоБольничномуЛисту.ДнейОплаты
ИЗ
	Документ.НачислениеПоБольничномуЛисту КАК НачислениеПоБольничномуЛисту
ГДЕ
	НачислениеПоБольничномуЛисту.ПериодРегистрации &gt;= &amp;НачалоПериода
	И НачислениеПоБольничномуЛисту.ПериодРегистрации &lt;= &amp;КонецПериода
	И НЕ НачислениеПоБольничномуЛисту.ПометкаУдаления
	И НачислениеПоБольничномуЛисту.Организация В
			(ВЫБРАТЬ
				ВТОрганизации.Организация
			ИЗ
				ВТОрганизации)</query>
	</dataSet>
	<parameter>
		<name>НачалоПериода</name>
		<title xsi:type="v8:LocalStringType">
			<v8:item>
				<v8:lang>ru</v8:lang>
				<v8:content>Начало периода</v8:content>
			</v8:item>
		</title>
		<valueType>
			<v8:Type>xs:dateTime</v8:Type>
			<v8:DateQualifiers>
				<v8:DateFractions>Date</v8:DateFractions>
			</v8:DateQualifiers>
		</valueType>
		<value xsi:type="xs:dateTime">0001-01-01T00:00:00</value>
		<useRestriction>true</useRestriction>
	</parameter>
	<parameter>
		<name>КонецПериода</name>
